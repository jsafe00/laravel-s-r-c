{
  "name": "safventure/laravel-s-r-c",
  "description": "CRUD Service-Repository-Controller",
  "license": "MIT",
  "keywords": [
    "laravel",
    "s",
    "r",
    "c"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Safventure",
      "email": "josafebalili@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Safventure\\laravelSRC\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Safventure\\laravelSRC\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Safventure\\laravelSRC\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
